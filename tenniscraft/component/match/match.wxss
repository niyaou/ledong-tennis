/* component/analyze/analyze.wxss */

.rowdiv {
  display: flex;
  flex-direction: row;
}


.columndiv {
  font-size: 26rpx;
  display: flex;
  flex-direction: column;
}

.analyzebanner {
  background: #fff;
  width: 100%;
  height: 112rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.analyzeicon {
  /* height: 100%; */
  width: 30%;
  align-items: center;
  flex-direction: row;
  justify-content: space-around;
  color: rgb(117, 117, 117);
}

.fontbold {
  font-size: 30rpx;
  font-weight: bolder;
}

.plus {
  margin-top:20rpx;
  width: 100%;
  height: 10%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  align-self: center;
  align-content: center;
}

.plusinner {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  align-self: center;
  align-content: center;
  border-radius: 20rpx;
  border-style:dashed;
  border-width: 2rpx;
  width: 80%;
  border-color: #abaeaf;
  background-color: #fff;
  height: 90%;
}