<!--component/playitem/playitem.wxml-->

<view class="rowdiv" bindtap="detail">
	<div class="{{doubleRankType0=='青铜'?'brass item':doubleRankType0=='白银'?'silver item':doubleRankType0=='黄金'?'golden item':'diamond item'}}">
		<div class="user">
			<div class="avator">
				<image bindtap="bindViewTap" class="userinfo-avatar" src="{{holderAvator}}" mode="cover" style="z-index:30"></image>
			</div>
      <div class="rest">  
      <div class="info"  style="width:30%">
            <div class="nickname">{{holderName}}		</div>
					<div class="rank">{{doubleRankType0}}</div>
      </div>
  
			<div class="zone" style="width:35%">
				<image src="../../icon/position.png" class="bottomimg" />
				<div> {{courtName}}</div>
			</div>
			<div class="zone"  style="width:35%">
				<image src="../../icon/rank.png" class="bottomimg" />
				<div> {{doubleWinRate}}% 胜率</div>
			</div></div>
  
		</div>
	</div>
</view>