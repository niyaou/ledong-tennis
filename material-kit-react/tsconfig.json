{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "strict": false,
      "noImplicitAny": false,
      "forceConsistentCasingInFileNames": true,
      "module": "es6",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "sourceMap": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "baseUrl": "src",
      
    },
    "include": ["src"]
  }
  